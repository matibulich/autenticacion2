generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql" // O el proveedor que estés usando
  url      = env("DATABASE_URL") // Asegúrate de que esta variable esté configurada en tu archivo .env
}

model user {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  password String
}